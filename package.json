{
  "name": "snap.hutao.manjusaka.docs",
  "version": "2.0.0",
  "description": "Documents site for Snap Hutao-Manjusaka project",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:build": "node generate-catalog.js && vitepress build docs",
    "docs:clean-dev": "vitepress dev docs --clean-cache",
    "docs:dev": "node generate-catalog.js && vitepress dev docs",
    "lint": "prettier --check --write .",
    "update:theme": "pnpm dlx vp-update",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/dom-view-transitions": "^1.0.6",
    "husky": "9.1.7",
    "markdown-it-container": "^4.0.0",
    "medium-zoom": "^1.1.0",
    "nano-staged": "0.8.0",
    "prettier": "3.4.2",
    "sass-embedded": "1.83.0",
    "vitepress": "^1.0.0",
    "vitepress-plugin-tabs": "^0.7.3",
    "vitepress-sidebar": "^1.33.1",
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "nano-staged": {
    "*.{json,md,ts,vue}": "prettier --write"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "tailwindcss": "^4.1.18"
  }
}